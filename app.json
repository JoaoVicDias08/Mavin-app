{
  "expo": {
    "name": "Mavin-app",
    "slug": "Mavin-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mavinapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.joaomuniz.Mavinapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "722ade5e-80ae-4118-874d-e2c7bd55dddc"
      }
    }
  }
}
